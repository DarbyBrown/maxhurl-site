{
  "name": "maxhurl-site",
  "version": "1.0.0",
  "description": "",
  "main": "static/js/canvas.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-js": "mkdir -p static/build/js && browserify -t babelify static/src/js/canvas.js -o static/build/js/canvas.js",
    "watch": "watchify -t babelify static/src/js/canvas.js -o static/build/js/canvas.js -v",
    "cp-images": "mkdir -p static/build/images && cp -r static/src/images/* static/build/images/ || :",
    "cp-fonts": "mkdir -p static/build/fonts && cp -r static/src/fonts/* static/build/fonts/ || :",
    "cp-css": "mkdir -p static/build/css && cp -r static/src/css/* static/build/css/ || :",
    "cp-cv": "mkdir -p static/build/cv && cp -r static/src/cv/* static/build/cv/ || :",
    "cp-robot": "cp static/src/robots.txt static/build/robots.txt || :",
    "build": "npm run build-js && npm run cp-robot && npm run cp-cv && npm run cp-css && npm run cp-fonts && npm run cp-images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maximilianhurl/maxhurl-site.git"
  },
  "author": "Max Hurl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maximilianhurl/maxhurl-site/issues"
  },
  "homepage": "https://github.com/maximilianhurl/maxhurl-site#readme",
  "dependencies": {
    "babel-cli": "=6.3.17",
    "babel-preset-es2015": "=6.3.13",
    "babelify": "=7.2.0",
    "matter-js": "=0.8.0",
    "poly-decomp": "^0.1.1",
    "underscore": "=1.8.3",
    "vectorize-text": "=3.0.2",
    "visibilityjs": "=1.2.1"
  },
  "devDependencies": {
    "watchify": "=3.6.1"
  }
}
